package Day0104;

public class review17 {

	public static void main(String[] args) {
		//변수는 args를 이용할 것
				/*
				 * 학생명 : 김선호 님
				 * 지역 : 부산
				 * 나이 : 23 세 
				 * 
				 * ================================================
				 * 김선호님의 자바점수는 88점 오라클점수는 77점 총 165점입니다.
				 * 
				 * */
		
		String name = args[0];
		String area = args[1];
		
		int age = Integer.parseInt(args[2]);
		int java = Integer.parseInt(args[3]);
		int oracle = Integer.parseInt(args[4]);
		
		int total = java + oracle;
		
		System.out.println("학생명 : "+name+"님");
		System.out.println("지역 : "+area);
		System.out.println("나이 : "+age+"세");
		
		System.out.println("김선호님의 자바점수는 "+ java+"점 오라클점수는 "+oracle+"점 총"+total+"점입니다");
		

	}

}
